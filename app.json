{
  "expo": {
    "name": "kan-images",
    "slug": "kan-images",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.khanhdang.kanimages"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "298d9a27-0ede-48fe-acaf-9f06fce63517"
      }
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Cho phép $(PRODUCT_NAME) truy cập vào ảnh",
          "savePhotosPermission": "Cho phép $(PRODUCT_NAME) lưu ảnh",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
